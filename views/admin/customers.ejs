        <div class="table-responsive">
          <table class="table table-sm card-table table-nowrap">
            <thead>
              <tr>
                
                <th>
                 Customer
                </th>
                <th>
                 Email
                </th>
                <th>
                 Phone
                </th>
                <th>
                 Address
                </th>
                
                <th>&nbsp;</th>
              </tr>
            </thead>
            <tbody class="list">
              <% data.forEach(async (customer)=> { %>
                <tr>
                  <td>
                    <span class="avatar avatar-sm me-3">
                      <img src="/assets_admin/img/uifaces/14.jpg" class="avatar-image rounded-circle" alt="">
                    </span>
                    <a href="#!" class="item-name text-dark relative-top-2">
                      <%= customer.user_name %>
                    </a>
                  </td>
                  <td class="item-email">
                    <%= customer.email %>
                  </td>
                  <td class="item-role">
                    <%= customer.phone %>
                  </td>
                  <td>
                    <%= customer.address %>
                  </td>
                  <td>
                    <div class="dropdown">
                      <a href="#" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="svg-icon text-gray-500">
                          <svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 512 512"><title>ionicons-v5-f</title><circle cx="256" cy="256" r="32" style="fill:none;stroke:#000;stroke-miterlimit:10;stroke-width:32px"/><circle cx="416" cy="256" r="32" style="fill:none;stroke:#000;stroke-miterlimit:10;stroke-width:32px"/><circle cx="96" cy="256" r="32" style="fill:none;stroke:#000;stroke-miterlimit:10;stroke-width:32px"/></svg>
                        </span>
                      </a>
                      <div class="dropdown-menu bg-pastel-purple" aria-labelledby="dropdownMenuButton">
                        <% if (customer.is_blocked) { %>

                        <a class="dropdown-item text-success" href="/blockUser/<%= customer.id %>">Unblock</a>

                        <% } else { %>
                          <a class="dropdown-item text-danger " href="/blockUser/<%= customer.id %>">Block</a>

                          <% } %>
                      </div>

                   

                    </div>
                  </td>
                </tr>
                <% }) %>
            </tbody>
            
            

            
