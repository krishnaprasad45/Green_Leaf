      <div class="main-content">
        


        <div class="container-fluid pb-7">






          <div class="row justify-content-center mt-5">
            <div class="col-12 col-xl-7">
              <h3 class="mb-3 mb-md-0">
                Update Product
              </h3>
              <form action="/update_product_post/<%= product._id %>" method="post">

                <div class="from-group mt-4 pt-3">
                  <label class="form-label">
                    Product name:
                  </label>
                  <input type="text" class="form-control yellow-border" value="<%=product.product_name %>"
                    name="product_name">
                </div>

                <div class="from-group mt-4">
                  <label class="form-label">
                    Project details:
                  </label>
                  <textarea rows="4" class="form-control yellow-border"
                    name="product_details"><%=product.product_details %></textarea>
                </div>

                <div class="from-group mt-4">
                  <label class="form-label">
                    Category:
                  </label>
                  <div class="input-group">
                    <input type="text" class="form-control yellow-border" value="<%=product.category %>"
                      name="category">
                  </div>
                </div>

                <div class="from-group mt-4">
                  <label class="form-label">
                    Price:
                  </label>
                  <div class="input-group">
                    <input type="text" class="form-control yellow-border" value="<%=product.price %>" name="price">
                  </div>
                </div>

                <!-- <div class="from-group mt-4">
              <label class="form-label mb-0">
